{
  "manifest_version": 2,
  "name": "Denmo 01",
  "short_name": "Keep tabs history",
  "version": "1.0.0",
  "description":
    "Mua hang ho",
  "background": {
    "scripts": ["js/jquery.min.js", "background/background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css":["css/style.css", "css/bootstrap.min.css"],
      "js": ["js/jquery.min.js", "js/bootstrap.min.js", "contents/config.js", "contents/tools.js", "contents/tmall.js", "contents/app.js"],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "tabs",
    "activeTab",
    "http://*/*",
    "https://*/*",
    "storage",
    "alarms"
  ],
  "minimum_chrome_version": "60",
  "icons": {
    "16": "assets/list.png",
    "48": "assets/list.png",
    "128": "assets/list128.png"
  },
  "content_security_policy":
    "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
